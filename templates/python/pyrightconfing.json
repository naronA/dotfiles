{
  "include": ["src", "tests", "seeder", "migrations", "deliciapp", "tools"],

  "exclude": ["**/node_modules", "**/__pycache__"],

  "ignore": [],

  "stubPath": "src/typestubs",
  "venvPath": ".venv",

  "reportMissingImports": true,
  "reportMissingTypeStubs": false,

  "pythonVersion": "3.7",
  "typeCheckingMode": "basic",
  "executionEnvironments": [
    {
      "root": "./",
      "pythonVersion": "3.7",
      "extraPaths": [
        "src/layer/utils/python",
        "src/api/auth_funcs",
        "src/api/line_funcs",
        "src/api/dashboard_funcs",
        "src/api/feature_funcs",
        "src/api/recipe_funcs",
        "src/api/system_funcs",
        "src/api/terms_funcs",
        "src/api/user_funcs",
        "src/api/fire_control_record_funcs"
      ]
    }
  ]
}
